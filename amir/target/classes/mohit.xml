<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd" default-autowire="default">

	<bean name="mohit" class="com.te.amir.amir.Amir"
		autowire="constructor" >

		<property name="name" value="mohit" />
		<property name="last_name" value="narkhede" />
		<property name="age" value="27" />
		
	</bean>
	<bean name="phone" class="com.te.amir.amir.Phone">
		<constructor-arg value="note 6 pro" />
		<constructor-arg value="mi" />
		<constructor-arg value="120222" />
	</bean>
</beans>