package com.te.packages;

public class LargestNo {
	public static void main(String[] args) {
		
	
	int x=10;
	int y=20;
	int z=30;
	int a=1;
	int b=3;
	if(x>y && x>z && x>a && x>b ) {
		System.out.println(x);
		}
		else if(y>z && y>a && y>b )
		{
			System.out.println(y);
	}
		else if(z>a && z>b)
		{
			System.out.println(z);
		}
		else if(a>b)
		{
			System.out.println(a);
			}}
		}
