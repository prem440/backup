package com.te.douts;

import java.util.Scanner;

public class Second {
static int r=0;
	public static void main(String[] args) {
		
	run();
	}
public static void run() {
	Scanner sc = new Scanner(System.in);
	int d = sc.nextInt();
	add(d);
}
	private static void add(int d) {
		r+=d;
		System.out.println(r);
		run();
		
    
	}

}
