package com.te.threads;

public class t4  extends Thread {
	@Override
	public void run() {
		System.out.println();
		for (int i = 0; i < 11; i++) {
			if(i==15)
			{
				
			}
			System.out.println(i+"---u2");
		}
		
		super.run();
	}
public static void main(String[] args) {
	Thread t=new t4();
	t.start();
	
}
	
	
	
	
	

}
