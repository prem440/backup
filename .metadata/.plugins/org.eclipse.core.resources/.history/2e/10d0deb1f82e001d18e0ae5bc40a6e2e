package com.te.ktask;

public class ocurence {
	static int d;
	static int b[];
	public static void main(String[] args) {
		int f[] = { 1, 2, 3, 2, 1, 4, 3 };
		
		for (int i = 0; i < f.length; i++) {
			b = new int[f.length];
			int a = i + 1;

			if (f[i] == a) {
				b[i] = f[i];
			}

		}
		for (int c = 0; c < b.length; c++) {
			System.out.print(b[i]);
		}
	}

}
