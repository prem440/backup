package com.te.threads;

public class test extends Thread {
	public static void main(String[] args) {
		         test t=new test();
		         t.start();
		for (int i = 0; i < 10;i++) {
			System.out.println(i);
		}
	}
	@Override
	public void run() {
		for (int i = 0; i < 10;i++)
				{
			System.out.println(i);
			
		}
	
	}

}
